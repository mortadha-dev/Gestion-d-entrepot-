<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="454.0" prefWidth="727.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.ReadStockageController">
   <children>
      <TableView fx:id="tabstock" layoutX="78.0" layoutY="27.0" onMouseClicked="#fill" prefHeight="200.0" prefWidth="526.0">
        <columns>
          <TableColumn fx:id="columnid" prefWidth="95.0" text="id" />
          <TableColumn fx:id="columnentrepot" prefWidth="93.0" text="Nom entrepot" />
            <TableColumn fx:id="columnproduit" prefWidth="111.0" text="Nom produit" />
            <TableColumn fx:id="columnquantite" prefWidth="114.0" text="Quantite" />
            <TableColumn fx:id="columndate" prefWidth="112.0" text="date de stockage" />
        </columns>
      </TableView>
      <Label layoutX="78.0" layoutY="259.0" text="Nom entrepot" />
      <Label layoutX="375.0" layoutY="259.0" text="Nom produit" />
      <Label layoutX="78.0" layoutY="319.0" text="Quantite" />
      <Label layoutX="375.0" layoutY="319.0" text="Date de stockage" />
      <ComboBox fx:id="comboentrepot" layoutX="173.0" layoutY="255.0" onAction="#importentrepot" prefWidth="150.0" />
      <ComboBox fx:id="comboproduit" layoutX="486.0" layoutY="255.0" onAction="#importproduit" prefWidth="150.0" />
      <TextField fx:id="quantites" layoutX="174.0" layoutY="315.0" />
      <TextField fx:id="dates" layoutX="487.0" layoutY="315.0" />
      <Button fx:id="ajouter" layoutX="50.0" layoutY="390.0" mnemonicParsing="false" onAction="#ajouterstock" text="Ajouter un stockage" />
      <Button fx:id="refresh" layoutX="212.0" layoutY="390.0" mnemonicParsing="false" onAction="#Refresherstock" text="Refresher le stockage" />
      <Button fx:id="modifier" layoutX="382.0" layoutY="390.0" mnemonicParsing="false" onAction="#modifierstock" text="Modifier le stockage" />
      <Button fx:id="supprimer" layoutX="545.0" layoutY="390.0" mnemonicParsing="false" onAction="#supprimerstock" text="Supprimer un stockage" />
   </children>
</AnchorPane>
